<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="productlabels" columnCount="3" pageWidth="595" pageHeight="842" columnWidth="187" leftMargin="2" rightMargin="30" topMargin="2" bottomMargin="20">
	<property name="ireport.scriptlethandling" value="0"/>
